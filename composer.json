{
    "name":        "league/tactician-container",
    "description": "Tactician integration with the PHP League DI container",
    "keywords":    ["tactician", "di", "container", "league"],
    "license":     "MIT",
    "authors": [
        {
            "name": "Nigel Greenway",
            "homepage": "http://futurepixels.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5",
        "league/tactician": "^0.6",
        "league/container": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Tactician\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Tactician\\Container\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
