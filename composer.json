{
    "name":        "league/tactician-container",
    "description": "Tactician integration for any container implementing PSR-11",
    "keywords":    ["tactician", "di", "container", "league", "container-interop", "interoperable"],
    "license":     "MIT",
    "authors": [
        {
            "name": "Nigel Greenway",
            "homepage": "http://futurepixels.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5",
        "league/tactician": "^1.0",
        "psr/container": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "squizlabs/php_codesniffer": "~2.0",
        "league/container": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Tactician\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Tactician\\Container\\Test\\": "tests/"
        }
    }
}
